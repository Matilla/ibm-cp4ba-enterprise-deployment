apiVersion: v1
kind: Route
metadata:
  name: kibana
spec:
  host: "kibana.{{ apps_endpoint_domain }}"
  to:
    kind: Service
    name: kibana
  tls:
    termination: edge
    insecureEdgeTerminationPolicy: Redirect
    key: |-
      {{ kibana_tls_key | b64decode | indent(6) }}
    certificate: |-
      {{ kibana_tls_crt | b64decode | indent(6) }}
    caCertificate: |-
      {{ kibana_ca_crt | b64decode | indent(6) }}